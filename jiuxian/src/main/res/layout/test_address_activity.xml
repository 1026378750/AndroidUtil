<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/background_color"
    android:orientation="vertical">

    <com.zxin.zxinlib.view.CommonCrosswiseBar
        android:id="@+id/ccb_beike_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:bar_background="@color/red_home_title"
        app:isTopBar="true"
        app:left_button_image="@drawable/icon_title_back_transparent"
        app:left_button_size="20px"
        app:show_left_button="true"
        app:title_text="标题"
        app:title_textColor="@color/color_ffffff"
        app:title_textSize="@dimen/size_18px" />

    <android.support.v4.widget.NestedScrollView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="BaseJXHomeApi"
                android:textColor="@color/black_25"
                android:textSize="20.0sp" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <EditText
                    android:id="@+id/BaseJXHomeApi"
                    android:layout_width="fill_parent"
                    android:layout_height="60.0dip"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center_vertical"
                    android:textColor="#ff60656d" />
            </LinearLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="BaseJXOmsApi"
                android:textColor="@color/black_25"
                android:textSize="20.0sp" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <EditText
                    android:id="@+id/BaseJXOmsApi"
                    android:layout_width="fill_parent"
                    android:layout_height="60.0dip"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center_vertical"
                    android:textColor="#ff60656d" />
            </LinearLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="BaseJXProductApi"
                android:textColor="@color/black_25"
                android:textSize="20.0sp" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <EditText
                    android:id="@+id/BaseJXProductApi"
                    android:layout_width="fill_parent"
                    android:layout_height="60.0dip"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center_vertical"
                    android:textColor="#ff60656d" />
            </LinearLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="BaseJXPromotionApi"
                android:textColor="@color/black_25"
                android:textSize="20.0sp" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <EditText
                    android:id="@+id/BaseJXPromotionApi"
                    android:layout_width="fill_parent"
                    android:layout_height="60.0dip"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center_vertical"
                    android:textColor="#ff60656d" />
            </LinearLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="BaseJXUserApi"
                android:textColor="@color/black_25"
                android:textSize="20.0sp" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <EditText
                    android:id="@+id/BaseJXUserApi"
                    android:layout_width="fill_parent"
                    android:layout_height="60.0dip"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center_vertical"
                    android:textColor="#ff60656d" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/cancel"
                    android:layout_width="0.0dip"
                    android:layout_height="60.0dip"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1.0"
                    android:text="DEVELOP"
                    android:textSize="20.0sp" />

                <Button
                    android:id="@+id/restart"
                    android:layout_width="0.0dip"
                    android:layout_height="60.0dip"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1.0"
                    android:text="restart_app"
                    android:textSize="20.0dip" />
            </LinearLayout>
        </LinearLayout>
    </android.support.v4.widget.NestedScrollView>
</LinearLayout>